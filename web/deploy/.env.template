# üåê Web Dashboard - Suporte Portal

# Build
NODE_ENV=production
PUBLIC_URL=/portal/suporte/
VITE_BASE_PATH=/portal/suporte/

# API
VITE_API_URL=https://office.inoveon.com.br/api/suporte
VITE_API_TIMEOUT=30000

# Aplica√ß√£o
VITE_APP_NAME=Suporte Dashboard
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DEBUG=false

# WebSocket (futuro)
VITE_WS_URL=wss://office.inoveon.com.br/api/suporte/ws

# Analytics (opcional)
VITE_GA_TRACKING_ID=
VITE_SENTRY_DSN=

# Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/*,application/pdf,.doc,.docx

# UI
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true

# Cache
VITE_CACHE_VERSION=1.0.0

# Build
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false